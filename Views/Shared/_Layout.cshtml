<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/library/Bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital@1&family=Roboto:wght@300&display=swap" rel="stylesheet">
    @RenderSection("Css", required: false)
</head>
<body class="wh-bg">
  

   


    <div class="container-fluid body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/library/Jquery/jquery-3.4.1.slim.min.js"></script>
    <script src="~/library/Bootstrap/popper.min.js"></script>
    <script src="~/library/Bootstrap/bootstrap.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
